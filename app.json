{
  "expo": {
    "name": "Mutual Fund Calculators",
    "slug": "MutualFundCalculators",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unishubh.gladiator"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "android": {
      "package": "com.unishubh.gladiator",
      "versionCode": 2
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}